-- {{ header }}
WITH
  _current AS (
    SELECT
      {% raw %}
      {% if is_init() %}
      MIN(submission_date) AS first_seen_date,
      {% else %}
      @submission_date AS first_seen_date,
      {% endif %}
      {% endraw %}
      * EXCEPT(submission_date, is_active),
    FROM
      `{{ project_id }}.{{ channel_dataset }}_derived.usage_reporting_clients_daily_v1`
    WHERE
      usage_profile_id IS NOT NULL
      {% raw %}
      {% if is_init() %}
      AND submission_date > "2014-10-10"
      GROUP BY
        usage_profile_id
      {% else %}
      AND submission_date = @submission_date
      {% endif %}
      {% endraw %}
  ),
_previous AS (
  SELECT
    usage_profile_id,
  FROM
    `{{ project_id }}.{{ channel_dataset }}_derived.{{ view_name }}_v1`
  WHERE
    {% raw %}
    {% if is_init() %}
    False
    {% else %}
    first_seen_date < @submission_date
    {% endif %}
    {% endraw %}
)

SELECT
  _current.*,
FROM
  _current
LEFT JOIN
  _previous
  USING (usage_profile_id)
WHERE
  _previous.usage_profile_id IS NULL
QUALIFY
  IF(
    COUNT(*) OVER (PARTITION BY usage_profile_id) > 1,
    ERROR("Duplicate usage_profile_id combination detected."),
    TRUE
  )
